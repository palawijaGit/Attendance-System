<?xml version="1.0" encoding="UTF-8"?>


<beans  xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-4.0.xsd">
    
	

   <bean id="addr" class="co.edureka.Address">
		<property name="city" value="Pelabuhan Klang"/>
		<property name="state" value="Selangor"/>
		<property name="zipCode" value="42000"/>
	</bean>
	
	<bean id="emp1" class="co.edureka.Employee">
		<property name= "eid" value="102"/>
		<property name= "ename" value="Habibi"/>
		<property name= "esalary" value="60000"/>
		<property name= "address" ref="addr"/>
	</bean>
	
	

</beans>